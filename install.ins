[Initial]
Message=Installing Autodesk Fusion 360...
DefVar $ExitCode$

[Actions]
ShowBitmap "%ScriptPath%\logo.png" "Autodesk Fusion 360"
WinBatch_Setup
Sub_HandleExitCode

[WinBatch_Setup]
"%ScriptPath%\data\Fusion 360 Admin Install.exe" --quiet

[Sub_HandleExitCode]
; check return code
Set $ExitCode$ = GetLastExitCode
comment "GetLastExitCode: " + $ExitCode$
if not($ExitCode$ = "0")
  LogError "Fatal: setup returned exit code " + $ExitCode$
  IsFatalError
endif